<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Final_Project"
        xmlns:Properties="clr-namespace:Final_Project.Properties" x:Class="Final_Project.Start"
        mc:Ignorable="d"
        Title="Start" Height="673.329" Width="852.729" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="Manual" Icon="Images/player.png">
    <Canvas x:Name="canvasDisplay" Background="Black">
        <Image x:Name="logo" Height="133" Canvas.Left="116" Canvas.Top="117" Width="618" OpacityMask="Black" Source="Images/logo.png" HorizontalAlignment="Center" VerticalAlignment="Center" Stretch="UniformToFill"/>
        <Button x:Name="button_Start" Content="New Game" Canvas.Left="116" Canvas.Top="420" Width="137" Height="49" Click="button_StartClick"/>
        <Button x:Name="button_Quit" Content="Exit" Canvas.Left="597" Canvas.Top="420" Width="137" Height="49" Click="button_QuitClick"/>
        <Button x:Name="button_Load" Content="Load Game" Canvas.Left="354" Canvas.Top="420" Width="136" Height="49" IsEnabled="False" Cursor="No"/>

    </Canvas>
</Window>
